{
  "manifest_version": 3,
  "name": "Pretty Pink Tax",
  "version": "1.1",
  "description": "Objection! Overruling the Pink Tax.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://www.flipkart.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.flipkart.com/*"],
      "js": ["content.js"], 
      "css": ["styles.css"]
    }
  ]
}